{
  "name": "expense-tracker-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js",
    "start": "ts-node-dev --respawn ./src/index.ts",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint './src/**/*.ts'",
    "format": "prettier --write './src/**/*.ts'",
    "migration:generate": "npm run typeorm -- migration:generate --dataSource ./src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/config/data-source.ts",
    "migration:create-migration": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ./src/config/data-source.ts",
    "test": "jest",
    "migration:create": "npm run typeorm -- migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "caller-path": "^4.0.0",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
